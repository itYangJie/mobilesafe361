<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:androidstu="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
   android:orientation="vertical"
    tools:context="com.mobilesafe.SeettingCenterActivity">
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="设置中心"
        android:textSize="35sp"
        android:background="@android:color/holo_blue_bright"
        android:alpha="0.65"
        android:gravity="center"/>
    <com.ui.SettingItemView
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    androidstu:titleString="设置自动检查更新"
    androidstu:desc_on="自动检查更新已开启"
    androidstu:desc_off="自动检查更新已关闭"
    android:id="@+id/set_update">

</com.ui.SettingItemView>
    <com.ui.SettingItemView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        androidstu:titleString="来电去电显示号码归属地"
        androidstu:desc_on="归属地显示已开启"
        androidstu:desc_off="归属地显示已关闭"
        android:id="@+id/set_numberAddress">

    </com.ui.SettingItemView>
   <com.ui.SettingClickView
       android:layout_width="match_parent"
       android:layout_height="wrap_content"
       androidstu:titleString="来电去电归属地显示框个性化"
       android:id="@+id/set_numberAddShowStyle"
       />
    <com.ui.SettingItemView
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    androidstu:titleString="黑名单拦截功能"
    androidstu:desc_on="黑名单拦截已开启"
    androidstu:desc_off="黑名单拦截已关闭"
    android:id="@+id/set_blackNumStop"/>

    <com.ui.SettingItemView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        androidstu:titleString="锁定应用程序"
        androidstu:desc_on="应用程序已锁定"
        androidstu:desc_off="应用程序已解除锁定"
        android:id="@+id/set_appLock"/>
    <TextView
        android:paddingTop="10dp"
        android:clickable="true"
        android:onClick="setPassword"
        android:paddingBottom="10dp"
        android:textColor="#000000"
        android:paddingLeft="10dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="left"
        android:textSize="20sp"
        android:text="修改手机防盗密码"/>
    <View
        android:layout_width="match_parent"
        android:layout_height="0.2dp"
        android:layout_marginLeft="6dp"
        android:layout_marginRight="6dp"
        android:background="#000000"/>
    <TextView
        android:paddingTop="10dp"
        android:clickable="true"
        android:onClick="setAppLockPwd"
        android:paddingBottom="10dp"
        android:textColor="#000000"
        android:paddingLeft="10dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="left"
        android:textSize="20sp"
        android:text="修改程序锁密码"/>
    <View
        android:layout_width="match_parent"
        android:layout_height="0.2dp"
        android:layout_marginLeft="6dp"
        android:layout_marginRight="6dp"
        android:background="#000000"/>
    <TextView
        android:paddingTop="10dp"
        android:clickable="true"
        android:onClick="unInstall"
        android:paddingBottom="10dp"
        android:textColor="#000000"
        android:paddingLeft="10dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="left"
        android:textSize="20sp"
        android:text="卸载应用程序"/>
    <View
        android:layout_width="match_parent"
        android:layout_height="0.2dp"
        android:layout_marginLeft="6dp"
        android:layout_marginRight="6dp"
        android:background="#000000"/>
</LinearLayout>
